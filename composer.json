{
    "name": "jackalope/jackalope-mongodb",
    "type": "library",
    "description": "Jackalope Transport library",
    "keywords": ["transport implementation", "mongodb", "phpcr"],
    "homepage": "http://jackalope.github.com",
    "license": [
        "MIT",
        "Apache-2.0"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Jackalope Community",
            "homepage": "https://github.com/jackalope/jackalope-mongodb/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpcr/phpcr": "~2.1.2",
        "jackalope/jackalope": "~1.2.0",
        "doctrine/mongodb": "~1.1.0"
    },
    "provide": {
        "jackalope/jackalope-transport": "1.1.0"
    },
    "require-dev": {
        "phpcr/phpcr-api-tests": "~2.1.0"
    },
    "autoload": {
        "psr-0": { "Jackalope\\": "src/" }
    },
    "bin": ["bin/jackalope"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
